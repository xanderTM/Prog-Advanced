<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiOefeningen.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MauiOefeningen.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiOefeningen">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="NaamTonenPage"
        ContentTemplate="{DataTemplate views:NaamTonenPage}"
        Route="NaamTonenPage" />

    <ShellContent
        Title="VakPage"
        ContentTemplate="{DataTemplate views:VakPage}"
        Route="VakPage" />

    <ShellContent
        Title="AfbeeldingenPage"
        ContentTemplate="{DataTemplate views:AfbeeldingenPage}"
        Route="AfbeeldingenPage" />
</Shell>
